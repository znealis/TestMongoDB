<!DOCTYPE HTML>
<style>
    .Headers{
        color:#92A8D1;
        justify-content:  center;
        align-items: center;
        text-align: center;
        font-size: 64px;
        text-shadow: -1px -1px 0 white, 1px -1px 0 white,-1px 1px 0 white,1px 1px 0 white;
    }

    .grid-container {
        display: grid;
        height: 630px;
        grid-template-columns: auto auto auto auto;
        grid-template-rows: auto auto;
        padding: 10px;
        overflow-y: scroll;
    }

    .grid-contanier > div {
        background: radial-gradient(#993955, #E9ECF5);
        color: white;
        padding: 20px;
        font-size: 20px;
        text-align: center;    
    }

    footer {
        text-align: center;
        padding: 3px;
        color: white;
        font-family:monaco;
    }

    a:link, a:visited {
        color: white;
        padding: 14px 25px;
        text-align: center;
        text-decoration: none;
        display: inline-block;
    }

   
    body{
        background-image: url(data:image/jpeg;base64,);
        background-repeat: repeat;
        background-size:25%;
        color: black;
        text-shadow: -1px -1px 0 gold, 1px -1px 0 gold,-1px 1px 0 gold,1px 1px 0 gold;
    }

    .form{
        padding: 8px;
    }

    li{
        float:left;
    }

    .list-fix{
        float: none;
        display: block;
        padding: 12px;
        text-align: center;
        /*background: radial-gradient(#993955, #E9ECF5);
        color:white;*/
        background: url(https://pngimg.com/uploads/bokeh/bokeh_PNG27.png);
        background-size: cover;
    }

    .img-container{
        position: relative;
    }

    .img-container:hover img{
        opacity: 0.3;
    }

    .details{
        transition: .5s ease;
        opacity: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
        text-align: center;
    }

    .text{
        font: 28px "Alternate Gothic";
    }

    .img-container:hover .details{
        opacity: 1;
    }

    img{
        float: none;
        display: block;
        margin-right: auto;
        margin-left: auto;
        opacity: 1;
        transition: .5 ease;
        backface-visibility: hidden;
    }

    li a{
        display: block;
        color: white;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
        font-family: monaco;
    }

    li a:hover:not(.active) {
        background-color: #111;
    }

    ul{
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
        background-color: #333;
    }
    .navbarContainer{
        font-family: monaco;
        align-items: center; 
        justify-content: center;
        text-align: center;
        color:rgb(177, 225, 234);
        border-radius: 2px;
        cursor: pointer;
        transition: all linear 1s;
        height:45px; 
        list-style-type: none;
        margin: 0;
        padding: 0;
    }

    .linkButton{
        background-color: red;
        border-radius: 12px;
        text-align: center;
        color: white;
        font-size: large;
        width: 200px;
    }

    .navbarButton:hover{
        font-family: monaco;
        transition: all linear .5s;
        background: radial-gradient(#e66465, #9198e5);
        color:rgb(255, 255, 255);
        border-radius: 5%;
        list-style-type: none;
        margin: 0;
        padding: 0;
    }
</style>

    <body>
        <div class="navbarContainer" id="navbarContainer">
            <li><a class="active" href="/" id="Home">Home</a></li>
            <li><a href="christina.ejs" id="christina">Shazam</a></li>
            <li><a href="nicole.ejs" id="nicole">AudioDB</a>
            <li><a href="john.ejs" id="john">Itunes</a></li>   
            <li><a href="zach.ejs" id="zach">Deezer</a></li>   
        </div>
        <h1 class="Headers" id="itunes-header">Top 25 Best iTunes Songs</h1>
      
        <div class = "grid-container">
            <% for(var i = 0; i < results.length; i++) {%>    
                <li class = "list-fix">
                    <div class="img-container">
                        <img src = '<%= results[i].songz.albumCover %>'/>
                        <div class="details">
                            <div class="text"><%= results[i].songz.title %><br></div>
                            <div><%= results[i].songz.artist %><br></div>
                            <div><%= results[i].songz.album %><br></div>
                        </div>
                    </div>
                    <form class="form" action='<%= results[i].songz.Link %>' method="get" target="_blank">
                        <button class= "linkButton" type="submit">Listen Here!</button>
                    </form>          
                </li>
            <%} %>
         </div>
    </body>
</HTML>
